# AWS Cognito Configuration
# Copy this file to .env and fill in your actual values

# Your Cognito User Pool ID (format: region_poolId, e.g., us-east-1_abc123def)
VITE_APP_COGNITO_USER_POOL_ID=

# Your Cognito App Client ID (long alphanumeric string)
VITE_APP_COGNITO_USER_POOL_CLIENT_ID=

# Your Cognito Domain (format: your-domain.auth.region.amazoncognito.com)
VITE_APP_COGNITO_DOMAIN=

# Base URL for your application (optional, used as fallback)
# For development: http://localhost:5173
# For production: https://your-domain.com
VITE_APP_BASE_URL=http://localhost:5173
